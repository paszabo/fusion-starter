{
  "name": "vin-react-starter-kit",
  "version": "4.0.0",
  "description": "Starter kit for creating React apps at Vinsolutions",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test:comment": "**NOTE: You need to manually add a glob for any tests nested more than 1 layer deep. Haven't found a way to search infinite depth via mocha command line**",
    "test": "mocha tools/testSetup.js src/**/*.spec.js src/components/**/*.spec.js --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.0",
    "bootstrap": "3.3.6",
    "object-assign": "4.0.1",
    "react": "15.0.1",
    "react-bootstrap": "0.29.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "redux": "3.4.0",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "browser-sync": "2.12.3",
    "chai": "3.5.0",
    "colors": "1.1.2",
    "connect-history-api-fallback": "1.2.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.7.0",
    "eslint-plugin-import": "1.4.0",
    "eslint-plugin-react": "4.3.0",
    "eslint-watch": "2.1.10",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "glob": "7.0.3",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "npm-run-all": "1.7.0",
    "react-addons-test-utils": "15.0.1",
    "redux-immutable-state-invariant": "1.2.2",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
