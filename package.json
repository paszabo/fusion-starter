{
  "name": "vin-react-starter-kit",
  "version": "3.0.1",
  "description": "Starter kit for creating React apps at Vinsolutions",
  "scripts": {
    "prestart": "npm run remove-dist",
    "start": "npm-run-all --parallel lint:tools test:watch open:src ",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "coverage": "cross-env NODE_ENV=test istanbul cover _mocha -- --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\"  --reporter min",
    "open-coverage": "npm run coverage && open coverage/lcov-report/index.html"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.0",
    "object-assign": "4.0.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.1",
    "redux": "3.3.1",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "browser-sync": "2.11.2",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "isparta": "4.0.0",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "npm-run-all": "1.6.0",
    "redbox-react": "1.2.2",
    "redux-immutable-state-invariant": "1.2.2",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "watch": "0.17.1",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.10.0",
    "yargs": "4.3.2"
  }
}
